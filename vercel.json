{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/sign-in",
      "dest": "index.js"
    },
    {
      "src": "/api/blogs",
      "dest": "index.js"
    },
    {
      "src": "/api/blogs/:id",
      "dest": "index.js"
    }
  ]
}

  